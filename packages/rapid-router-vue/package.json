{
  "name": "@rapid/router-vue",
  "version": "0.0.0",
  "description": "Vue composables for Rapid Router with auto-intercepting navigation",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bunup --format esm --dts --target node --minify",
    "clean": "bun rimraf dist .turbo"
  },
  "keywords": ["rapid", "router", "routing", "vue", "composable", "navigation"],
  "author": "Sylphx <hi@sylphx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/rapid.git",
    "directory": "packages/rapid-router-vue"
  },
  "homepage": "https://github.com/SylphxAI/rapid/tree/main/packages/rapid-router-vue#readme",
  "bugs": {
    "url": "https://github.com/SylphxAI/rapid/issues"
  },
  "peerDependencies": {
    "@rapid/router-core": "^0.0.0",
    "@rapid/signal-core": "^0.0.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@rapid/signal": "workspace:*",
    "@rapid/router-core": "workspace:*",
    "@rapid/signal-core": "workspace:*",
    "bunup": "^0.16.10",
    "typescript": "^5.8.3",
    "vue": "^3.5.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
