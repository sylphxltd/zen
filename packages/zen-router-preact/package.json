{
  "name": "@sylphx/zen-router-preact",
  "version": "5.0.2",
  "description": "Preact integration for @sylphx/zen-router",
  "author": "Sylphx <hi@sylphx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/zen.git",
    "directory": "packages/zen-router-preact"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "zen",
    "router",
    "preact",
    "hooks",
    "state management"
  ],
  "homepage": "https://github.com/SylphxAI/zen/tree/main/packages/zen-router-preact#readme",
  "bugs": {
    "url": "https://github.com/SylphxAI/zen/issues"
  },
  "scripts": {
    "build": "bunup --format esm,cjs --dts --target node --minify",
    "clean": "bun rimraf dist .turbo"
  },
  "dependencies": {
    "@sylphx/zen": "workspace:*",
    "@sylphx/zen-router": "workspace:*"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "bunup": "^0.15.13",
    "@types/react": "^18.3.3",
    "preact": "^10.22.1",
    "typescript": "^5.4.5",
    "@preact/preset-vite": "^2.8.2",
    "@testing-library/preact": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
