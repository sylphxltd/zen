{
  "name": "@sylphx/zen-router",
  "version": "5.0.8",
  "description": "Tiny URL router for zen state management.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunup --format esm,cjs --dts --target node --minify",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "zen",
    "router",
    "routing",
    "state management",
    "minimal",
    "tiny"
  ],
  "author": "Sylphx <hi@sylphx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/zen.git",
    "directory": "packages/zen-router"
  },
  "homepage": "https://github.com/SylphxAI/zen/tree/main/packages/zen-router#readme",
  "bugs": {
    "url": "https://github.com/SylphxAI/zen/issues"
  },
  "dependencies": {
    "@sylphx/zen": "workspace:*",
    "@sylphx/zen-patterns": "workspace:*"
  },
  "devDependencies": {
    "bunup": "^0.15.13",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
