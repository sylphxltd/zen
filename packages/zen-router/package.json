{
  "name": "@sylphx/zen-router",
  "version": "1.0.1",
  "description": "Tiny URL router for zen state management.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bunup --format esm,cjs --dts --target node --minify"
  },
  "keywords": ["zen", "router", "routing", "state management", "minimal", "tiny"],
  "author": "Sylph Lab <hi@sylphlab.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylphx/zen.git",
    "directory": "packages/zen-router"
  },
  "homepage": "https://github.com/sylphx/zen/tree/main/packages/zen-router#readme",
  "bugs": {
    "url": "https://github.com/sylphx/zen/issues"
  },
  "devDependencies": {
    "bunup": "^0.15.13",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sylphx/zen": "^1.2.1"
  }
}
