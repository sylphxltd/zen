{
  "name": "@zen/signal-svelte",
  "version": "0.0.0",
  "description": "Svelte stores integration for Zen Signal reactive primitives",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "sideEffects": false,
  "scripts": {
    "build": "bunup --format esm,cjs --dts --target node --minify"
  },
  "dependencies": {
    "@zen/signal": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "bunup": "^0.15.13",
    "typescript": "^5.4.5",
    "svelte": "^5.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/zen.git",
    "directory": "packages/zen-signal-svelte"
  },
  "keywords": ["zen", "signal", "reactive", "svelte", "stores"],
  "author": "Sylphx <hi@sylphx.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
