{
  "name": "@rapid/babel-plugin-jsx",
  "version": "0.1.0",
  "description": "Babel plugin for Rapid JSX transform with lazy children evaluation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bunup src/index.ts --format cjs --dts --target node",
    "dev": "bunup src/index.ts --format cjs --watch",
    "test": "vitest"
  },
  "keywords": ["rapid", "jsx", "babel", "transform", "reactive"],
  "author": "SylphxAI",
  "license": "MIT",
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/babel__core": "^7.20.5",
    "bunup": "^0.16.10",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
